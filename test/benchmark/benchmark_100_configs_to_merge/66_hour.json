                        "Config": {
                            "ConnectionLimit": 209,
                            "path": "/relate/where/recently.socket",
                            "EventBlackLiist": ".event.severity 2 GE ",
                            "authorizedProcesses": [
                                ".process.gid 54257 LE AND .process.gid 54257 LE OR .proc.gid 54257 GT ",
